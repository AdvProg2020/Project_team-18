            xmlns:fx="http://javafx.com/fxml" fx:controller="graphics.ProductMenu$CompareMenu">
    <center>
        <TableView prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <columns>
                <TableColumn id="nameColumn" prefWidth="75.0" text="Name" />
                <TableColumn id="priceColumn" prefWidth="75.0" text="Price" />
                <TableColumn id="sellerColumn" prefWidth="75.0" text="Seller" />
                <TableColumn id="rateColumn" prefWidth="104.0" text="Average Rate" />
                <TableColumn id="brandColumn" prefWidth="64.0" text="Brand" />
                <TableColumn id="explanationColumn" prefWidth="112.0" text="Explanation" />
                <TableColumn id="supplyColumn" prefWidth="94.0" text="Supply" />
            </columns>
        </TableView>
    </center>
    <bottom>
        <Button alignment="CENTER" mnemonicParsing="false" onAction="#back" prefHeight="27.0" prefWidth="65.0" text="Back" BorderPane.alignment="CENTER" />
    </bottom>
</BorderPane>
