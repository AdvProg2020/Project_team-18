<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<?import java.net.URL?>
<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="graphics.AdminMangeProducts">
    <stylesheets>
        <URL value="@cssSheets/BasicStyle.css" />
    </stylesheets>
    <stylesheets>
        <URL value="@cssSheets/BackGround.css" />
    </stylesheets>
    <children>
        <Button layoutX="14.0" layoutY="20.0" mnemonicParsing="false" onAction="#back" text="back" />
        <TableView fx:id="productTable" layoutX="86.0" prefHeight="524.0" prefWidth="514.0">
            <columns>
                <TableColumn fx:id="nameColumn" prefWidth="75.0" text="ProductId" />
                <TableColumn fx:id="imageColumn" prefWidth="76.0" text="Image" />
                <TableColumn fx:id="categoryColumn" prefWidth="70.0" text="Category" />
                <TableColumn fx:id="priceColumn" prefWidth="56.0" text="Price" />
                <TableColumn fx:id="averageRateColumn" prefWidth="88.0" text="Average rate" />
                <TableColumn fx:id="sellerColumn" prefWidth="56.0" text="Seller" />
                <TableColumn fx:id="buttonColumn" prefWidth="72.0" text="View More" />
            </columns>
        </TableView>
        <Button onAction="#removeProduct" layoutX="270.0" layoutY="533.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="130.0" text="Remove Product" />
    </children>
</AnchorPane>
